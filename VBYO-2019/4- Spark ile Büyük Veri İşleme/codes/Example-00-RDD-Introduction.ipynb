    "from pyspark import SparkContext\n",
    "sc = SparkContext.getOrCreate()\n",
    "\n",
    "arr = [1,2,3,4,5,6,7,8]\n",
    "rdd1 = sc.parallelize(arr)\n",
    "\n",
    "def kareAl(a):\n",
    "    return a * a\n",
    "\n",
    "\n",
    "rdd2 = rdd1.map(lambda x : x * x )\n",
    "\n",
    "sonArray = rdd2.collect()\n",
    "\n",
    "print(sonArray)\n",
    "\n"
   ]
